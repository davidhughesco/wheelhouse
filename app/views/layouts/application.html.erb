<!DOCTYPE html>
<html>
	<head>
		<title>WheelCreator <%= yield(:title) %></title>
		<meta name="Description" content="<%= yield(:description) %>">
		<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
		<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
		<%= csrf_meta_tags %>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<!-- Google Font and FontAwesome-->
		<link href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,700' rel='stylesheet' type='text/css'>
		<script src="https://use.fontawesome.com/553dd3931e.js"></script>
		<!-- SVG to Canvas to Image -->
		<script async="" type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js"></script>
		<script async="" type="text/javascript" src="http://canvg.github.io/canvg/rgbcolor.js"></script>
		<script async="" type="text/javascript" src="http://canvg.github.io/canvg/StackBlur.js"></script>
		<script async="" type="text/javascript" src="http://canvg.github.io/canvg/canvg.js"></script>
		<script async="" type="text/javascript" src="https://hongru.github.io/proj/canvas2image/canvas2image.js"></script>
	</head>
	<body>

		<%= render 'layouts/header' %>

		<%= flash_messages %>

		<%= yield %>

		<%= render 'layouts/footer' %>

	</body>
</html>
